            "@/views/*":["views/*"],
            "@/modules/*":["modules/*"],
            "@/styles/*":["styles/*"],
            "@/config/*":["config/*"],
            "@/context/*":["context/*"],
            "@/helpers/*":["helpers/*"],
        },
        "module": "commonjs",
